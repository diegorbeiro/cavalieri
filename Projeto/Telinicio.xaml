<Page x:Class="Projeto.Telinicio"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Projeto"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="Telinicio">

    <Grid>
        <Viewbox Stretch="Fill">
            <Grid Width="800" Height="600">
                <Image Source="/Fazer Login (3).png" Stretch="Fill"/>
                <Button x:Name="Login" Content="Login" Background="Red" BorderBrush="{x:Null}" Click="Login_Click" Width="108" Height="38" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="181,448,0,0"/>
                <Button x:Name="Logins" Content="Entrar" Background="Red" BorderBrush="{x:Null}" Click="Login_Click" Width="108" Height="38" HorizontalAlignment="Center" VerticalAlignment="Top" Margin="0,449,0,0"/>
                <Button x:Name="Logins_Copiar" Content="Entrar" Background="Red" BorderBrush="{x:Null}" Click="Login_Click" Width="107" Height="38" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="509,449,0,0"/>
            </Grid>
        </Viewbox>
    </Grid>
</Page>
