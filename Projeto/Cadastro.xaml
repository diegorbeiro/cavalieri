<Page x:Class="Projeto.Cadastro"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Projeto"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="Cadastro">

    <Grid>
        <Viewbox Stretch="Fill">
            <Grid Width="800" Height="600">
                <Image Source="/bg.jpg" Stretch="Fill"/>
                <Button x:Name="Logins" Content="Entrar" Background="Red" BorderBrush="{x:Null}" Click="Login_Click" Width="108" Height="38" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="238,469,0,0" Opacity="0"/>
                <Button x:Name="Logins_Copiar" Content="Entrar" Background="Red" BorderBrush="{x:Null}" Click="Login_Click" Width="107" Height="38" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="429,469,0,0" Opacity="0"/>
                <Button x:Name="Voltar" Content="Voltar" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Height="40" Width="75" Click="Inicio_Click" FontFamily="Arial Black" FontWeight="Bold" FontSize="16" Background="{x:Null}" BorderBrush="{x:Null}" Opacity="0"/>
            </Grid>
        </Viewbox>
    </Grid>
</Page>
